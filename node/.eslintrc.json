{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-restricted-syntax": [
      "warn"
    ],
    "no-unused-vars": [
      "warn"
    ],
    "consistent-return": [
      "warn"
    ],
    "no-param-reassign": [
      "off"
    ],
    "camelcase": [
      "off"
    ],
    "no-mixed-operators": [
      "off"
    ],
    "no-plusplus": [
      "off"
    ],
    "no-underscore-dangle": [
      "off"
    ]
  }
}